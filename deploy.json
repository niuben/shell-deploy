{
  "actions": [
    {
      "name": "commit",
      "infor": "提交代码",
      "steps": [
        {
          "type": "git-pull"
        },
        {
          "type": "git-commit",
          "message": "提交说明"
        }
      ]
    },
    {
      "name": "upload",
      "infor": "上传服务器",
      "steps": [
        {
          "type": "upload",
          "ip": "服务器地址（可以是IP地址）",
          "user": "用户名",
          "password": "密码",
          "path": "指定服务器上传路径",
          "source": "本地的文件路径"
        }
      ]
    }
  ]
}